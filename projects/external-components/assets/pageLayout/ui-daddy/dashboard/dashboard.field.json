                  {
                    "actionType": "OPEN_MODAL",
                    "showLoading": true,
                    "target": "edit-project-modal"
                  },
                  {
                    "actionType": "WAIT_IN_MS",
                    "timer": 100
                  },
                  {
                    "actionType": "SET_SHARED_DATA",
                    "sharedData": [
                      {
                        "varName": "tableRowData",
                        "staticData": "$ROW_DATA$"
                      }
                    ]
                  },
                  {
                    "actionType": "setfield",
                    "field": "$editProjectDesc$",
                    "fieldValue": "$SHARED_DATA$.tableRowData.description",
                    "condition": "true",
                    "formName": "editProject"
                  },
                  {
                    "actionType": "setfield",
                    "field": "$projectId$",
                    "fieldValue": "$SHARED_DATA$.tableRowData.id",
                    "condition": "true",
                    "formName": "editProject"
                  },
                  {
                    "actionType": "setfield",
                    "field": "$editBranchName$",
                    "fieldValue": "$SHARED_DATA$.tableRowData.branchName",
                    "condition": "true",
                    "formName": "editProject"
                  },
                  {
                    "actionType": "setfield",
                    "field": "$editProjectTitle$",
                    "fieldValue": "$SHARED_DATA$.tableRowData.title",
                    "condition": "true",
                    "formName": "editProject"
                  },
                  {
                    "actionType": "setfield",
                    "field": "$editGitUrl$",
                    "fieldValue": "$SHARED_DATA$.tableRowData.forkUrl",
                    "condition": "true",
                    "formName": "editProject"
                  }
                ],
                "debounce": 3000
              }
            ],
